<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd >
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>de.matthias-endler.dtouchclient</string>
    <key>Program</key>
    <string>/usr/bin/dtouchclient.py</string>
    <key>KeepAlive</key>
    <true/>
    <!--
    If you like, you can pipe the script output to a logfile
    (great for debugging).
    <key>StandardErrorPath</key>
    <string>/path/to/your/dtouchclient.log</string>
    <key>StandardOutPath</key>
    <string>/path/to/your/dtouchclient.log</string>
    -->
  </dict>
</plist>
